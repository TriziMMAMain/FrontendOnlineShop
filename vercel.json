{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "optimize": {
          "include": [
            "@vue/runtime-core",
            "@vue/compiler-sfc",
            "vue",
            "lodash"
          ]
        },
        "devCommand": "yarn dev",
        "buildCommand": "yarn build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/robots.txt",
      "destination": "https://vue-robot.vercel.app/src/robot.html"
    }
  ]
}